"VertexLitGeneric"
{
	$baseTexture "models/zerosiki/alienswarm/wingstill_color"
	$bumpmap "models/zerosiki/alienswarm/wingstill_normal"

	"$phong"				"1"
	"$phongexponent" 		"20"
	"$phongboost"			"10"
	"$phongfresnelranges"	"[0.1 0.5 1]"
	"$phongexponenttexture" "models/zerosiki/alienswarm/exponent"
	"$phongalbedotint"		"1"

	$detail "models/zerosiki/alienswarm/model_layer_ice_1.vtf"
	$detailscale 5
	$detailblendfactor 0.0
	$detailblendmode 5

	$envmap "env_map"
	$envmaptint "[0.05 0.05 0.05]"
	$translucent 1

	$selfillum 1
	$selfIllumFresnel 1
	$selfIllumFresnelMinMaxExp "[0.1 1.25 5.0]" 
	$selfillumtint "[.5 1 1]"
	$selfillummask "models/zerosiki/alienswarm/buzzer_selfillum"

	Proxies
	{
		AnimatedTexture
		{
		animatedtexturevar $detail
		animatedtextureframenumvar $detailframe
		animatedtextureframerate 30
		}
	}
}